<resources xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="notAnimation">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>

    <style name="Theme.notAnimation" parent="android:Theme.Light.NoTitleBar.Fullscreen">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/notAnimation</item>
    </style>

    <style name="scenario_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="scenario_layout_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>

    <style name="scenario_layout_imge">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="left_view">
        <item name="android:background">@color/mainpage_noselect_bkg</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/left_button_size</item>
    </style>

    <style name="left_image">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
    </style>

    <style name="top_textview">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="top_imageview">
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>

    <style name="top_linerlayout">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="goodwhat">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="alarmzone_textview_bold">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="alarmzone_textview_normal">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <!--name是我们在使用时要用到的资源的标志，parent是指当前的样式所继承的父类样式-->
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item><!--隐藏标题栏-->
        <item name="android:background">#00000000</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="call_textview">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/call_text_transparent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="listview_divider">
        <item name="android:divider">#C6C6C6</item>
        <item name="android:dividerHeight">2px</item>
        <item name="android:textColor">@color/call_text_transparent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="listview_title">
        <item name="android:textColor">@color/black_grey</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="listview_content">
        <item name="android:textColor">@color/black_grey</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="notification_navi">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textSize">20sp</item>
    </style>


    <style name="notification_underline">
        <item name="android:background">@color/button_activity_blue</item>
        <item name="android:layout_height">6dp</item>
    </style>

    <style name="title_underline_gray">
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/line_color</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:layout_gravity">bottom</item>
    </style>

    <style name="SwitchButtonStyle">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>


    <style name="SwitchButtonStyle_KSW">
        <item name="com.honeywell.homepanel:kswAutoAdjustTextPosition">true</item>
        <item name="com.honeywell.homepanel:kswThumbDrawable">@mipmap/alarmzone_switch_circle</item>
        <item name="com.honeywell.homepanel:kswBackMeasureRatio">1.8</item>
        <item name="com.honeywell.homepanel:kswThumbHeight">35dp</item>
        <item name="com.honeywell.homepanel:kswThumbMargin">-1dp</item>
        <item name="com.honeywell.homepanel:kswThumbWidth">35dp</item>
        <item name="com.honeywell.homepanel:kswBackDrawable">@drawable/drawable_back_color</item>
    </style>

    <style name="ImageViewButton_Back">
        <item name="android:layout_marginLeft">-7dp</item>
        <item name="android:padding">3dp</item>
        <item name="android:scaleType">center</item>
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="zone_abnormal_adapter">
        <item name="android:textSize">20sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Dialog_FullScreen">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="layout_top_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:background">#F5F5F5</item>
    </style>

    <style name="dialog_button_style1">
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/normal_front_size</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="button_cancle">
        <item name="android:text">@string/indicator_board_cancel</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/normal_front_size</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:background">@drawable/standard_buttons_color</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="button_ok">
        <item name="android:text">@string/OK</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/normal_front_size</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:background">@drawable/primary_buttons_color</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>


    <style name="HoneywellTitleStyle">
        <item name="android:textColor">@color/black_grey</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_MEDIUM</item>
    </style>

    <style name="HoneywellTextUpStyle">
        <item name="android:textColor">@color/black_grey</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_BOLD</item>
    </style>


    <style name="HoneywellTextWifi">
        <item name="android:textColor">@color/black_grey</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_LIGHT</item>
    </style>

    <style name="HoneywellTextLightStyle">
        <item name="android:textColor">@color/black_grey</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_LIGHT</item>
    </style>

    <style name="HoneywellTextDownStyle">
        <item name="android:textColor">@color/normal_grey</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_BOOK</item>
    </style>

    <style name="HoneywellTextDowndownStyle">
        <item name="android:textColor">@color/normal_grey</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_BOOK</item>
    </style>

    <style name="HoneywellGridViewStyle">
        <item name="android:textColor">@color/black_grey</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_BOOK</item>
    </style>

    <style name="HoneywellButtonStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_BOLD</item>
    </style>

    <style name="HoneywellCallTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/call_text_transparent</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_BOLD</item>
    </style>

    <style name="HoneywellwhiteTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/call_text_transparent</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_BOLD</item>
    </style>

    <style name="HoneywellPwdTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/call_text_transparent</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="com.honeywell.homepanel:textStyle">HONEYWELL_FONTS_BOLD</item>
    </style>

</resources>
